<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>Configuration settings for the Lattice for Leads application.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>account_matching_field__c</fullName>
        <defaultValue>&quot;id&quot;</defaultValue>
        <deprecated>false</deprecated>
        <description>This field is used to specify the field in Salesforce that links accounts to salesPRISM.

NOTE: This field must correspond to an existing Salesforce Account object field. If the field doesn&apos;t exist, then the link back to salesPRISM will not work.</description>
        <externalId>false</externalId>
        <inlineHelpText>This field is used to specify the field in Salesforce that links accounts to salesPRISM.

NOTE: This field must correspond to an existing Salesforce Account object field. If the field doesn&apos;t exist, then the link back to salesPRISM will not work.</inlineHelpText>
        <label>Account Matching Field</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>default_tab__c</fullName>
        <defaultValue>&quot;BuyingSignals&quot;</defaultValue>
        <deprecated>false</deprecated>
        <description>This will determine which tab is shown initially when viewing the details of a Lead. Possible values are BuyingSignals or TalkingPoints. Default value is BuyingSignals.</description>
        <externalId>false</externalId>
        <inlineHelpText>This will determine which tab is shown initially when viewing the details of a Lead. Possible values are BuyingSignals or TalkingPoints. Default value is BuyingSignals.</inlineHelpText>
        <label>Default Tab</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>engage_button_label__c</fullName>
        <defaultValue>&quot;Engage&quot;</defaultValue>
        <deprecated>false</deprecated>
        <description>The label for the &quot;Engage&quot; button on the Account List view.

NOTE: This field is no longer used as the visualization has changed.</description>
        <externalId>false</externalId>
        <label>Engage Button Label</label>
        <length>10</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>no_data_message__c</fullName>
        <defaultValue>&quot;Lattice Insights have not been published for this record. For any questions, please contact your Lattice Support team.&quot;</defaultValue>
        <deprecated>false</deprecated>
        <description>Message displayed in Buyer Insights when there is no data found for a corresponding Salesforce Object.</description>
        <externalId>false</externalId>
        <inlineHelpText>Message displayed in Buyer Insights when there is no data found for a corresponding Salesforce Object.</inlineHelpText>
        <label>No Data Found Message</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>no_plays_message__c</fullName>
        <defaultValue>&quot;There are currently no plays for this account.&quot;</defaultValue>
        <deprecated>false</deprecated>
        <description>Message displayed when an Account has no plays.</description>
        <externalId>false</externalId>
        <inlineHelpText>Message displayed when an Account has no plays.</inlineHelpText>
        <label>No Plays Message</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>show_lattice_rating__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Show &quot;Highest&quot;, &quot;High&quot;, &quot;Medium&quot;, &quot;Low&quot; instead of a numeric score for the Lead</description>
        <externalId>false</externalId>
        <inlineHelpText>Show &quot;Highest&quot;, &quot;High&quot;, &quot;Medium&quot;, &quot;Low&quot; instead of a numeric score for the Lead</inlineHelpText>
        <label>Show Lattice Rating</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>show_lift__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>This will determine whether the Lift for Buying signals is displayed or if it will simply say Above/Below average. Default setting is unchecked.</description>
        <externalId>false</externalId>
        <inlineHelpText>This will determine whether the Lift for Buying signals is displayed or if it will simply say Above/Below average. Default setting is unchecked.</inlineHelpText>
        <label>Show Lift</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>show_score__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>Whether to show the score in the tile. Default value is checked.</description>
        <externalId>false</externalId>
        <inlineHelpText>Whether to show the score in the tile. Default value is checked.</inlineHelpText>
        <label>Show Score</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>support_email_address__c</fullName>
        <deprecated>false</deprecated>
        <description>Support email address to accompany the No Data Found Message when Buyer Insights doesn&apos;t find account information for a corresponding Salesforce Object.</description>
        <externalId>false</externalId>
        <inlineHelpText>Support email address to accompany the No Data Found Message when Buyer Insights doesn&apos;t find account information for a corresponding Salesforce Object.</inlineHelpText>
        <label>Support Email Address</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Email</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>url__c</fullName>
        <defaultValue>&quot;https://pls.lattice-engines.com/Dante3/index.aspx&quot;</defaultValue>
